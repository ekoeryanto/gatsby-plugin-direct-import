{
  "name": "gatsby-plugin-direct-import",
  "description": "Gatsby plugin to use Babel plugin direct import and make it tree shaking",
  "version": "1.0.0",
  "author": "Eko Eryanto <ekoeryanto@gmail.com>",
  "bugs": {
    "url": "https://github.com/ekoeryanto/gatsby-plugin-direct-import/issues"
  },
  "dependencies": {
    "babel-plugin-direct-import": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5"
  },
  "homepage": "https://github.com/ekoeryanto/gatsby-plugin-direct-import#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "babel-direct-import",
    "tree-shaking"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^1.0.0"
  },
  "repository": "https://github.com/ekoeryanto/gatsby-plugin-direct-import",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  }
}
